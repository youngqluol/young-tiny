{
  "name": "young-tiny",
  "type": "module",
  "version": "1.0.3",
  "description": "tiny image",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youngqluol/young-tiny.git"
  },
  "keywords": [
    "tinypng"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "tiny": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.8",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.12.12",
    "bumpp": "^9.4.1",
    "chalk": "^5.3.0",
    "eslint": "^9.3.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "md5": "^2.3.0",
    "minimist": "^1.2.8",
    "ora": "^8.0.1",
    "pnpm": "^9.1.1",
    "rimraf": "^5.0.7",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
